# 服务器配置
PORT=3001
NODE_ENV=development

# 数据库配置
DB_HOST=localhost
DB_PORT=5432
DB_NAME=tron_energy_rental
DB_USER=postgres
DB_PASSWORD=password

# Redis配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# JWT配置
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_EXPIRES_IN=24h
JWT_REFRESH_SECRET=your-super-secret-refresh-key-change-this-in-production
JWT_REFRESH_EXPIRES_IN=7d

# TRON网络配置
TRON_NETWORK=shasta
TRON_FULL_NODE=https://api.shasta.trongrid.io
TRON_SOLIDITY_NODE=https://api.shasta.trongrid.io
TRON_EVENT_SERVER=https://api.shasta.trongrid.io
TRON_PRIVATE_KEY=your-tron-private-key-here
TRON_ADDRESS=your-tron-address-here

# Telegram Bot配置
TELEGRAM_BOT_TOKEN=your-telegram-bot-token-here
TELEGRAM_WEBHOOK_URL=https://your-domain.com/api/telegram/webhook

# 系统配置
ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD=admin123456
ENCRYPTION_KEY=your-encryption-key-for-sensitive-data

# 日志配置
LOG_LEVEL=info
LOG_FILE_PATH=./logs

# 能量租赁配置
DEFAULT_ENERGY_PRICE=100
MIN_ENERGY_AMOUNT=1000
MAX_ENERGY_AMOUNT=1000000
ENERGY_POOL_SIZE=10000000

# 代理系统配置
DEFAULT_COMMISSION_RATE=0.1
MIN_COMMISSION_RATE=0.05
MAX_COMMISSION_RATE=0.3